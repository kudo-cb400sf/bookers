<h2>Books</h2>

<!--ここに投稿一覧を表示させたい。-->

<h3>New book</h3>

<div>
  <%= flash[:notice] %>
</div>

<%= form_with model: @book, url: '/books', method: :post do |f| %>
  
  <h6>Title</h6>
  <%= f.text_field :title %>
  
  <h6>Body</h6>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>
  
<% end %>